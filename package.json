{
  "name": "dealership-websites",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "The dealership-website is  based multi domain web application that uses the Nuxt3 framework",
  "author": "LiteBreeze Infotech Pvt Ltd",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/cardia/dealership-websites.git"
  },
  "scripts": {
    "build": "nuxt build",
    "build:staging": "nuxt build --dotenv .env.staging",
    "build:production": "nuxt build --dotenv .env.production",
    "dev": "nuxt dev --port 5005",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "bundle": "node ./tools/bundler.mjs",
    "pack:staging": "npm run build:staging && npm run bundle -- --tag=staging",
    "pack:production": "npm run build:production && npm run bundle -- --tag=production",
    "pack:marketing": "npm run build && npm run bundle -- --tag=marketing",
    "lint": "eslint .  --ignore-path .gitignore .",
    "lint:fix": "eslint . --fix  --ignore-path .gitignore .",
    "prettier:fix": "prettier --write --list-different . --ignore-path .gitignore ."
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.7",
    "@popperjs/core": "^2.11.6",
    "@typescript-eslint/parser": "^5.57.0",
    "aws-amplify": "^6.0.20",
    "basic-auth": "^2.0.1",
    "bootstrap": "^5.3.0-alpha1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "pinia": "^2.0.32",
    "prettier": "^2.8.7",
    "tsscmp": "^1.0.6",
    "vue-multiselect": "^3.0.0-beta.1",
    "vue3-carousel": "^0.2.9"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^0.12.1",
    "@aws-amplify/backend-cli": "^0.11.1",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/i18n": "^8.0.0-beta.12",
    "@types/basic-auth": "^1.1.3",
    "@types/bootstrap": "^5.2.6",
    "@types/lodash": "^4.14.191",
    "@types/tsscmp": "^1.0.0",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "archiver": "^5.3.1",
    "aws-cdk": "^2.132.1",
    "aws-cdk-lib": "^2.132.1",
    "constructs": "^10.3.0",
    "eslint": "^8.35.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "nuxt": "^3.2.0",
    "nuxt-svgo": "^0.0.3",
    "sass": "^1.58.1",
    "typescript": "^5.4.2"
  },
  "overrides": {
    "vue": "latest"
  }
}
